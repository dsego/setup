[user]
	name = Davorin Å ego
	email = davorin.sego@gmail.com
[color]
	ui = auto
[alias]
	co = checkout
	br = branch
	ci = commit
	cp = cherry-pick
	st = status
	up = push -u origin HEAD
	ll = log --pretty=format:'%C(magenta)%h%Creset -%C(bold yellow)%d%Creset %s %Cgreen(%cr) %C(bold cyan)<%an>%Creset'
	lg = log --graph --pretty=format:'%C(magenta)%h%Creset -%C(bold yellow)%d%Creset %s %Cgreen(%cr) %C(bold cyan)<%an>%Creset'
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[credential]
	helper = store
[core]
	excludesfile = /Users/davorin/.gitignore
